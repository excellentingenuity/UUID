{
  "name": "eig/eloquent-uuid",
  "description": "UUID allowing easy generation and assignment for event sourced systems",
  "license": "MIT",
  "keywords": ["uuid", "event sourcing"],
  "type": "library",
  "homepage": "https://github.com/excellentingenuity/UUID",
  "authors": [
    {
      "name": "James Johnson",
      "email": "james.johnson@excellentingenuity.com",
      "role": "Developer/Maintainer"
    }
  ],
  "support": {
    "docs": "https://github.com/excellentingenuity/UUID",
    "issues": "https://github.com/excellentingenuity/UUID/issues",
    "wiki": "https://github.com/excellentingenuity/UUID/wiki"
  },
  "require": {
    "php": ">=5.6",
    "ramsey/uuid": "^3.2",
    "moontoast/math": "^1.1",
    "ircmaxell/random-lib": "^1.1"

  },
  "require-dev": {
    "phpunit/phpunit": "~5.6",
    "mockery/mockery": "^0.9.4",
    "satooshi/php-coveralls": "dev-master",
    "apostle/phpunit-validation": "1.0.*@dev"
  },
  "autoload": {
    "psr-4": {
      "eig\\UUID\\": "./src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "eig\\UUID\\Tests\\": "./tests"
    }
  }
}